Config { font = "xft:Helvetica-12"
	, bgColor = "black"
	, fgColor = "grey"
	, position = TopW L 85
	, lowerOnStart = True
	, commands = [ 
                  Run Weather "KLAX" ["-t"," <tempF>F","-L","64","-H","77","--normal","green","--high","red","--low","lightblue"] 36000
                , Run Network "eth0" ["-t", "<fc=#ffff00><dev>:</fc> <rx>|<tx>"] 10
                , Run Network "wlan0" ["-t", "<fc=#ffff00><dev>:</fc> <rx>|<tx>"] 10
		, Run Cpu ["-t","<fc=#ffff00>Cpu:</fc> <total>","-L","3","-H","50","--normal","green","--high","red"] 10
		, Run Memory ["-t","<fc=#ffff00>Mem:</fc> <usedratio>%"] 10
		, Run Swap ["-t","<fc=#ffff00>Swap:</fc> <usedratio>" ] 10
		, Run Date "%a %b %_d %l:%M" "date" 10
		, Run StdinReader
		, Run CommandReader "/home/apallatto/.xmonad/ledmon" "LED"

		]
	, sepChar = "%"
	, alignSep = "}{"
	, template = "%StdinReader% }{ %cpu% %memory% %swap% %eth0% %wlan0% <fc=#ee9a00>%date%</fc> %KLAX%"
       }
