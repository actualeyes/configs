Config { font = "xft:Helvetica-10"
	, bgColor = "black"
	, fgColor = "grey"
	, position = TopW L 85
	, lowerOnStart = True
	, commands = [ 
                  Run Weather "RJOB" ["-t"," <tempF>F","-L","64","-H","77","--normal","green","--high","red","--low","lightblue"] 36000
                , Run Network "enp10s0" ["-t", "<fc=#ffff00><dev>:</fc> <rx>|<tx>"] 10
                , Run Network "wlp3s0" ["-t", "<fc=#ffff00><dev>:</fc> <rx>|<tx>"] 10
		, Run MultiCpu ["-t","<fc=#ffff00>Cpu:</fc> <total>","-L","3","-H","50","--normal","green","--high","red"] 10
                , Run BatteryP ["BAT0"] [
                                "-t", "<fc=#ffff00>Batt:</fc> <left>% / <timeleft>",
                                "-L","10","-H","80",
                                "-l", "red", "-h", "green"
                                ]
                               50
		, Run Memory ["-t","<fc=#ffff00>Mem:</fc> <usedratio>%"] 10
		, Run Swap   ["-t","<fc=#ffff00>Swap:</fc> <usedratio>%"] 10
		, Run Date "%a %b %_d %l:%M" "date" 10
		, Run StdinReader
		, Run CommandReader "/home/apallatto/.xmonad/ledmon" "LED"

		]
	, sepChar = "%"
	, alignSep = "}{"
	, template = "%StdinReader% }{ %battery% %multicpu% %memory% %swap% %enp10s0% %wlp3s0% <fc=#ee9a00>%date%</fc> %RJOB%"
       }
